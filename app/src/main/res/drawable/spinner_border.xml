<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape android:shape="rectangle">
            <corners android:radius="16dp" />

            <stroke
                android:width="1dp"
                android:color="#FFFFFF"
            />
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_spinner_icon"
        android:end="12dp"
        android:right="12dp"
        android:bottom="12dp"
        android:top="12dp"
        android:start="12dp"
        android:gravity="right" />
</layer-list>
